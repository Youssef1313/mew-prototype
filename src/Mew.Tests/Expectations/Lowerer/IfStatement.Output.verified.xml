<?xml version="1.0" encoding="utf-8"?>
<Statements>
  <Let Name="corgi" Value="3" />
  <ConditionalGoto If="corgi == 3" Is="False" Goto="Label1" />
  <FunctionCall Name="print">
    <Argument Type="string">Three</Argument>
  </FunctionCall>
  <Goto Name="Label2" />
  <Label Name="Label1" />
  <FunctionCall Name="print">
    <Argument Type="string">Not three!</Argument>
  </FunctionCall>
  <Label Name="Label2" />
</Statements>